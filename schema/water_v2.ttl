# baseURI: http://localhost/ns/water-monitor
# imports: http://localhost/ns/data
# imports: http://www.w3.org/ns/ssn

@prefix wo: <http://localhost/ns/water-monitor#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn:  <http://www.w3.org/ns/ssn/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ssn-ext: <http://www.w3.org/ns/ssn/ext/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix op: <http://localhost/cv/op/> .
@prefix data: <http://localhost/ns/data> .
@prefix cv: <http://localhost/cv/water> .

<http://localhost/ns/water-monitor>
 rdf:type owl:Ontology ;
 owl:imports <http://purl.org/dc/elements/1.1/> ;
 owl:imports ssn: ;
 owl:imports ssn-ext: ;
.

wo:hasBarrier 
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf owl:topObjectProperty ;
  owl:inverseOf wo:isBarrierOf
.

wo:hasSimpleEvent
 rdf:type owl:ObjectProperty ;
 rdfs:subPropertyOf owl:topObjectProperty 
.

wo:isBarrierOf
 rdf:type owl:ObjectProperty ;
 rdfs:subPropertyOf owl:topObjectProperty 
.

wo:isEventOf
 rdf:type owl:ObjectProperty ;
 rdfs:subPropertyOf owl:topObjectProperty 
.

wo:hasEvents
 rdf:type owl:ObjectProperty ;
 rdfs:subPropertyOf owl:topObjectProperty 
.

ssn-ext:hasMember
  rdf:type owl:ObjectProperty ;
.  

ssn-ext:isMemberOf
  rdf:type owl:ObjectProperty ;
  rdfs:domain [
      rdf:type owl:Class ;
      owl:unionOf (
          sosa:Observation
          wo:ComplexWaterEvent
        ) ;
    ] ;
  rdfs:label "is member of" ;
  rdfs:range wo:ComplexWaterEvent ;
  owl:inverseOf ssn-ext:hasMember ;
.

wo:BarrierName
  rdf:type owl:DatatypeProperty ;
  rdfs:label "Barrier" ;
  rdfs:range xsd:string ;
.

wo:siteName
  rdf:type owl:DatatypeProperty ;
  rdfs:label "Site" ;
  rdfs:range xsd:string ;
.

wo:Barrier
 rdf:type owl:Class ;
 rdfs:label "Barrier" ;
 rdfs:subClassOf 
 [ 
     rdf:type owl:Restriction ;
              owl:onProperty wo:isBarrierOf ;
              owl:allValuesFrom wo:WaterMonitoringSite
 ]  ,
 [
      rdf:type owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty wo:BarrierName 
 ] ;
.


wo:ChemicalProperty 
  rdf:type owl:Class ;
  rdfs:subClassOf sosa:ObservableProperty 
.

wo:ComplexEventNotification
 rdf:type owl:Class ;
 rdfs:subClassOf ssn:Output 
.


wo:Event 
  rdf:type owl:Class ;
.

wo:PhysicalProperty
 rdf:type owl:Class ;
 rdfs:subClassOf sosa:ObservableProperty 
.

wo:WaterMonitoringSite
 rdf:type owl:Class ;
 rdfs:subClassOf sosa:Sample ,
 [ 
   rdf:type owl:Restriction ;
   owl:onProperty wo:hasBarrier ;
   owl:someValuesFrom wo:Barrier
 ] ,
 [
   rdf:type owl:Restriction ;
   owl:minCardinality "1"^^xsd:nonNegativeInteger ;
   owl:onProperty wo:siteName ;
 ] 
.

wo:ComplexWaterEvent rdf:type owl:Class ;
  rdfs:subClassOf wo:Event ,
  [ 
    rdf:type owl:Restriction ;
    owl:onProperty sosa:hasFeatureOfInterest ;
    owl:allValuesFrom wo:WaterMonitoringSite
  ],
  [
    rdf:type owl:Restriction ;
    owl:onProperty ssn:hasOutput ;
    owl:allValuesFrom wo:ComplexEventNotificatoin
  ],
  [
    rdf:type owl:Restriction ;
    owl:onProperty ssn-ext:hasMember ;
    owl:allValuesFrom wo:WaterObservation
  ] ,
  [
   rdf:type owl:Restriction ;     
   owl:onProperty sosa:resultTime ;
   owl:allValuesFrom xsd:dateTime;
  ] , 
  [
  	rdf:type owl:Restriction ;
    owl:onProperty sosa:observedProperty ;
    owl:allValuesFrom sosa:ObservableProperty
  ] ,
  [
    rdf:type owl:Restriction ;     
    owl:minCardinality "1"^^xsd:nonNegativeInteger;
   	owl:onProperty sosa:resultTime 
  	 
  ]
.

wo:IndustrialDischarge 
  rdf:type owl:Class ;
  rdfs:subClassOf wo:ComplexWaterEvent 
.

wo:WaterObservation 
  rdf:type owl:Class ;
  rdfs:subClassOf sosa:Observation ,
  [ 
    rdf:type owl:Restriction ;
    owl:onProperty sosa:hasFeatureOfInterest ;
    owl:allValuesFrom wo:WaterMonitoringSite
  ],
  [ 
    rdf:type owl:Restriction ;
    owl:onProperty ssn-ext:isMemberOf ;
    owl:someValuesFrom wo:ComplexWaterEvent
  ]
  
  .